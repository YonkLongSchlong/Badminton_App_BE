{
  "name": "badminton_app_be",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun index.ts",
    "dev": "bun --watch index.ts",
    "db:generate": "drizzle-kit generate --dialect=postgresql --schema src/db/schema/index.ts --out=src/db/migrations",
    "db:migrate": "bun run src/db/migrate.ts",
    "db:seed": "bun run/src/db/seed.ts",
    "build": "bun build index.ts --compile --outfile server"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/nodemailer": "^6.4.16",
    "drizzle-kit": "^0.24.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.688.0",
    "@aws-sdk/credential-provider-ini": "^3.687.0",
    "@hono/swagger-ui": "^0.5.0",
    "@hono/zod-validator": "^0.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/pg": "^8.11.10",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.6.1",
    "install": "^0.13.0",
    "json-server": "^1.0.0-beta.3",
    "mock-aws-s3": "^4.0.2",
    "nock": "^13.5.6",
    "nodemailer": "^6.9.15",
    "openai": "^4.73.1",
    "pg": "^8.12.0",
    "postgres": "^3.4.4",
    "redis": "^4.7.0",
    "stripe": "^17.3.1",
    "totp-generator": "^1.0.0",
    "zod": "^3.23.8"
  }
}
