{
  "name": "badminton_app_be",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun index.ts",
    "dev": "bun --watch index.ts",
    "db:generate": "drizzle-kit generate --dialect=postgresql --schema ./src/db/schema/index.ts --out=./src/db/migrations",
    "db:migrate": "bun run ./src/db/migrate.ts",
    "db:seed": "bun run ./src/db/seed.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.24.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/pg": "^8.11.10",
    "bcrypt": "^5.1.1",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.6.1",
    "pg": "^8.12.0",
    "redis": "^4.7.0",
    "totp-generator": "^1.0.0",
    "zod": "^3.23.8"
  }
}
