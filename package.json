{
  "name": "badminton_app_be",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun index.ts",
    "dev": "bun --watch index.ts",
    "db:generate": "drizzle-kit generate --dialect=postgresql --schema ./src/db/schema.ts --out=./src/db/migrations",
    "db:migrate": "bun run ./src/db/migrate.ts",
    "db:seed": "bun run ./src/db/seed.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.24.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.2.2",
    "drizzle-orm": "^0.33.0",
    "hono": "^4.6.1",
    "postgres": "^3.4.4",
    "zod": "^3.23.8"
  }
}
